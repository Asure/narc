#
.SUFFIXES : .asm .obj .tbl .out
#
OBJS = NARC.OBJ NARC1.OBJ NARCBGND.OBJ NARCBULL.OBJ NARCLOOT.OBJ NARC6.OBJ \
 NARCPLAY.OBJ NARCTEXT.OBJ IMGTBL.OBJ NARCOL.OBJ NARCPAL.OBJ \
 NARCRAM.OBJ NARCSND.OBJ NARCCAD.OBJ NARCMISC.OBJ NARCPIMP.OBJ \
 NARCGANG.OBJ NARCHYPO.OBJ NARCUTIL.OBJ NARCDYNA.OBJ NARCBUG.OBJ \
 NARCHOOK.OBJ NARCPALL.OBJ NARCCARS.OBJ NARCDUMP.OBJ NARCDATA.OBJ \
 NARCSCOR.OBJ NARCCHOP.OBJ NARCPORS.OBJ BGNDPAL.OBJ BGNDTBL.OBJ \
 NARCANIM.OBJ NARCLOWN.OBJ NARCBLIP.OBJ

.asm.obj:
	echome "$*.ASM(1) ----Assembling----"
        c:\bin\gspa $* -h

narc: $(OBJS)
	echome "NARC ---- Linking"
        gsplnk narc.cmd
	tv narc

banzail:        $(OBJS)
                l68 -q game
                echo loading...
                c:\soft\xtalk\xtalk gamel


NARC.OBJ: NARC.ASM 
NARC1.OBJ: NARC1.ASM 
NARCBGND.OBJ: NARCBGND.ASM 
NARCBULL.OBJ: NARCBULL.ASM 
