#
.SUFFIXES : .exe .asm .obj .c .mot
#
OBJS = 	ROMCHIPS.OBJ RAMCHIPS.OBJ ROMBOARD.OBJ CPUBOARD.OBJ TVMONITR.OBJ CONTROLS.OBJ \
	MISCDATA.OBJ SYSFONT.OBJ SYSCHECK.OBJ ROMCHECK.OBJ RAMCHECK.OBJ	TVMCHECK.OBJ \
	CTLCHECK.OBJ UTILITIE.OBJ

.asm.obj:
        ECHO *----assembling----$*.ASM(0)
        GSPA $* -H -L -v10

ALL: $(OBJS)
	GSPLNK SYSCHECK.CMD
	GSPLNK CHECK512.CMD

$(OBJS): $*.ASM
